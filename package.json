{
    "name": "equicord",
    "private": "true",
    "version": "1.12.3",
    "description": "The other cutest Discord client mod",
    "homepage": "https://github.com/roothheo/bashcord#readme",
    "bugs": {
        "url": "https://github.com/roothheo/bashcord/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/roothheo/bashcord.git"
    },
    "license": "GPL-3.0-or-later",
    "author": "Equicord",
    "scripts": {
        "build": "node --require=./scripts/suppressExperimentalWarnings.js scripts/build/build.mjs",
        "buildStandalone": "pnpm build --standalone",
        "buildWeb": "node --require=./scripts/suppressExperimentalWarnings.js scripts/build/buildWeb.mjs",
        "buildWebStandalone": "pnpm buildWeb --standalone",
        "buildReporter": "pnpm buildWebStandalone --reporter --skip-extension",
        "buildReporterDesktop": "pnpm build --reporter",
        "watch": "pnpm build --watch",
        "dev": "pnpm watch",
        "watchWeb": "pnpm buildWeb --watch",
        "generatePluginJson": "tsx scripts/generatePluginList.ts",
        "generateEquicordPluginJson": "tsx scripts/generateEquicordPluginList.ts",
        "generateTypes": "tspc --emitDeclarationOnly --declaration --outDir packages/vencord-types --allowJs false",
        "inject": "node scripts/runInstaller.mjs -- --install",
        "uninject": "node scripts/runInstaller.mjs -- --uninstall",
        "lint": "eslint",
        "lint-styles": "stylelint \"src/**/*.css\" --ignore-pattern src/userplugins",
        "lint:fix": "pnpm lint --fix",
        "test": "pnpm buildStandalone && pnpm testTsc && pnpm lint:fix && pnpm lint-styles && pnpm generatePluginJson",
        "testWeb": "pnpm lint && pnpm buildWeb && pnpm testTsc",
        "testTsc": "tsc --noEmit"
    },
    "dependencies": {
        "@ffmpeg/ffmpeg": "^0.12.15",
        "@ffmpeg/util": "^0.12.2",
        "@intrnl/xxhash64": "^0.1.2",
        "@sapphi-red/web-noise-suppressor": "0.3.5",
        "@types/less": "^3.0.8",
        "@types/stylus": "^0.48.43",
        "@types/tinycolor2": "^1.4.6",
        "@vap/core": "0.0.12",
        "@vap/shiki": "0.10.5",
        "fflate": "^0.8.2",
        "gifenc": "github:mattdesl/gifenc#64842fca317b112a8590f8fef2bf3825da8f6fe3",
        "idb": "8.0.3",
        "jsqr": "1.4.0",
        "monaco-editor": "^0.52.2",
        "nanoid": "^5.1.5",
        "openai": "^5.5.1",
        "socket.io": "^4.8.1",
        "usercss-meta": "^0.12.0",
        "virtual-merge": "^1.0.2"
    },
    "devDependencies": {
        "@electron/asar": "^4.0.0",
        "@stylistic/eslint-plugin": "^4.4.1",
        "@types/chrome": "^0.0.326",
        "@types/diff": "^8.0.0",
        "@types/lodash": "^4.17.18",
        "@types/node": "^24.0.3",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@types/yazl": "^3.3.0",
        "diff": "^8.0.2",
        "discord-types": "^1.3.26",
        "esbuild": "^0.25.5",
        "eslint": "9.29.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-simple-header": "^1.2.2",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "highlight.js": "11.11.1",
        "html-minifier-terser": "^7.2.0",
        "moment": "^2.30.1",
        "puppeteer-core": "^24.10.2",
        "standalone-electron-types": "^34.2.0",
        "stylelint": "^16.20.0",
        "stylelint-config-standard": "^38.0.0",
        "ts-patch": "^3.3.0",
        "ts-pattern": "^5.7.1",
        "tsx": "^4.20.3",
        "type-fest": "^4.41.0",
        "typed-emitter": "^2.1.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.1",
        "typescript-transform-paths": "^3.5.5",
        "zip-local": "^0.3.5",
        "zustand": "^5.0.5"
    },
    "packageManager": "pnpm@10.4.1",
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "eslint-plugin-import",
                "eslint"
            ]
        },
        "allowedDeprecatedVersions": {
            "source-map-resolve": "*",
            "resolve-url": "*",
            "source-map-url": "*",
            "urix": "*",
            "q": "*"
        },
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    },
    "engines": {
        "node": ">=18"
    }
}